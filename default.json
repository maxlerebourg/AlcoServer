{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":npm"],
  "prConcurrentLimit": 4,
  "branchPrefix": "fix/TE-000-",
  "commitMessagePrefix": "fix: :sparkles: TE-000 ",
  "groupName": "all",
  "osvVulnerabilityAlerts": true,
  "packageRules": [
    {
      "matchPackagePatterns": ["@monisnap/serverless-moni-obs"],
      "allowedVersion": ">=1.0.11"
    },
    {
      "packagePatterns": ["*"],
      "enabled": false
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
